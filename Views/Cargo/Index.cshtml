@model IEnumerable<HubHumano.Models.cargo>

@{
    ViewBag.Title = "Index";
}

<br />
<h2>Cargos:</h2>

<p>
    @Html.ActionLink("Crear Cargo", "Create", "", new { @class = "btn btn-outline-primary" })
</p>
<table class="table">
    <tr>
        <th>
           Cargo:
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cargo1)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.id }) |
            @Html.ActionLink("Ver", "Details", new { id=item.id }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
